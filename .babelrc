{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "loose": true,
        "browsers": [
          "> 1%",
          "last 2 versions",
          "Firefox ESR"
        ]
      }
    ],
    "react"
  ],
  "plugins": [
    "external-helpers",
    "transform-class-properties",
    [
      "transform-es2015-spread",
      {
        "loose": true
      }
    ],
    "transform-object-rest-spread",
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true
      }
    ]
  ],
  "env": {
    "commonjs": {
      "presets": [
        [
          "env",
          {
            "loose": true
          }
        ]
      ],
      "plugins": ["babel-plugin-dynamic-import-node"]
    }
  }
}
